{% extends "base.json" %}
{% set schema = "unittest" %}
{% block groupby %}
  "groupby": [
    {
      "name": "platform",
      "value": "treeherder.machine.platform"
    }
  ],
{% endblock groupby %}
{% block where %}
  "where": {
    "and": [
      {
        "gt": {
          "run.stats.start_time": {
            "date": "{{ since }}"
          }
        }
      }
    ]
  },
{% endblock %}
