{% extends "tests_by_date.json" %}
{% set schema = "unittest" %}
{% block where %}
  "where": {
    "and": [
      {
        "eq": {
          "result.ok": "F"
        }
      },
      {% if branch %}
        {
          "eq": {
            "build.branch": "{{ branch }}"
          }
        },
      {% endif %}
      {
        "gt": {
          "run.stats.start_time": {
            "date": "{{ since }}"
          }
        }
      }{% if path %},
        {
          "prefix": {
            "result.test": "{{ path }}"
          }
        }
      {% endif %}
    ]
  },
{% endblock %}
