{% extends "tests_by_date.json" %}
{% set schema = "fx-test" %}
{% block where %}
  "where": {
    "and": [
      {
        "eq": {
          "result.ok": "F"
        }
      },
      {% if job %}
        {
          "eq": {
            "run.job_name": "{{ job }}"
          }
        },
      {% endif %}
      {
        "gt": {
          "run.stats.start_time": {
            "date": "{{ since }}"
          }
        }
      }{% if project %},
        {
          "prefix": {
            "run.job_name": "{{ project }}"
          }
        }
      {% endif %}
    ]
  },
{% endblock %}
